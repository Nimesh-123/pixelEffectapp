<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.pixel_effect_app">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/pixelphoto"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pixelphoto"
        android:supportsRtl="true"
        android:theme="@style/Theme.Pixel_Effect_app"
        tools:targetApi="31">
        <activity
            android:name=".Activity.Share_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.ImageEdit_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.crop_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.Creation"
            android:exported="false" />
        <activity
            android:name=".Activity.Start_Activity"
            android:exported="false" />
        <activity
            android:name=".Activity.home"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>